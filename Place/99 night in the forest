local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/tbao143/Library-ui/refs/heads/main/Redzhubui"))()
Library.Themes.Darker["Color Hub 2"] = Color3.fromRGB(20,20,20)
Library.Themes.Darker["Color Stroke"] = Color3.fromRGB(15,15,15)
Library.Themes.Darker["Color Theme"] = Color3.fromRGB(0,255,255)
Library.Themes.Darker["Color Text"] = Color3.fromRGB(255,255,255)
Library.Themes.Darker["Color Dark Text"] = Color3.fromRGB(0,255,255)
Library.Themes.Darker["Color Hub 1"] = ColorSequence.new({
 ColorSequenceKeypoint.new(0.00, Color3.fromRGB()),
 ColorSequenceKeypoint.new(0.50, Color3.fromRGB(5,5,5)),
 ColorSequenceKeypoint.new(1.00, Color3.fromRGB(2,2,2))
})

local Window = Library:MakeWindow({"Blueway", "v0.0.1", "testando | redz lib v5.lua"})

local Button = Window:AddMinimizeButton({
Button = { ImageColor3 = Color3.fromRGB(0,255,255), Image = "rbxassetid://95869322194132", BackgroundColor3 = Color3.fromRGB(), BackgroundTransparency = 0 },
Corner = { CornerRadius = UDim.new(0, 5) },
})
Instance.new("UIStroke", game:GetService("CoreGui")["redz Library V5"].Hub).Color = Color3.fromRGB(0,255,255)
Instance.new("UIStroke", game:GetService("CoreGui")["redz Library V5"].ImageButton).Color = Color3.fromRGB(0,255,255)

local Tabs = {
Main = Window:MakeTab({"Main", "Home"}),
Bring = Window:MakeTab({"Bring", "Book"}),
Visual = Window:MakeTab({"Visual", "Eye"}),
Teleport = Window:MakeTab({"Teleport", "Apple"})
}

Tabs.Main:AddSection({"Kill Aura"})
getgenv().killdistance = 50
game:GetService("RunService").RenderStepped:Connect(function()
for _,v in ipairs(workspace.Characters:GetChildren()) do
if getgenv().killaura and v.Parent.Name == "Characters" then
if (v:FindFirstChildOfClass("BasePart").Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < getgenv().killdistance then
game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("ToolDamageObject"):InvokeServer(v, game:GetService("Players").LocalPlayer:WaitForChild("Inventory"):FindFirstChildWhichIsA("Model"), "2_4241172567", CFrame.new(1,1,1))
end
end
end
end)
Tabs.Main:AddSlider({
Name = "Kill Distance",
Min = 50,
Max = 300,
Increase = 1,
Default = 50,
Callback = function(v)
getgenv().killdistance = v
end})
Tabs.Main:AddToggle({
Name = "Kill Aura",
Callback = function(v)
getgenv().killaura = v
end})

Tabs.Main:AddSection({"Chop Tree Aura"})

Tabs.Main:AddDropdown({
Name = "Tree List",
Options = {"Small Tree", "Big Tree"},
Default = "Small",
Callback = function(v)
getgenv().treelist = v or "Small Tree"
end})
Tabs.Main:AddToggle({
Name = "Chop Tree Aura",
Callback = function(v)
getgenv().choptreeaura = v
while getgenv().choptreeaura do
if getgenv().treelist == "Small Tree" then
for _,v in ipairs(workspace.Map:GetDescendants()) do
if v.Name == "Small Tree" and v then
game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("ToolDamageObject"):InvokeServer(v, game:GetService("Players").LocalPlayer:WaitForChild("Inventory"):FindFirstChildWhichIsA("Model"),"1_4241172567", CFrame.new(1,1,1))
end
end
elseif getgenv().treelist == "Big Tree" then
for _,v in ipairs(workspace.Map:GetDescendants()) do
if v.Name == "Big Tree1" or v.Name == "Big Tree2" or v.Name == "Big Tree3" and v then
game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("ToolDamageObject"):InvokeServer(v, game:GetService("Players").LocalPlayer:WaitForChild("Inventory"):FindFirstChildWhichIsA("Model"),"1_4241172567", CFrame.new(1,1,1))
end
end
end
task.wait(0.5)
end
end})

