







local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/ImamGV/Bacon-Library/refs/heads/main/bacon.txt"))()

for _,v in ipairs(workspace:GetDescendants()) do
if v:IsA("BasePart") and v.Color == Color3.fromRGB(255,0,0) then
v.CanTouch = false
elseif v:IsA("BasePart") and v.Color == Color3.fromRGB(255,51,0) then
v.CanTouch = false
elseif v:IsA("BasePart") and v.Color == Color3.fromRGB(0,0,255) then
v.CanTouch = false
end
end
workspace.DescendantAdded:Connect(function(v)
if v:IsA("BasePart") and v.Color == Color3.fromRGB(255,0,0) then
v.CanTouch = false
elseif v:IsA("BasePart") and v.Color == Color3.fromRGB(255,51,0) then
v.CanTouch = false
elseif v:IsA("BasePart") and v.Color == Color3.fromRGB(0,0,255) then
v.CanTouch = false
end
end)

local Tabs = {
Main = Library:CreateWindow("UGC Games")
}
Library:CreateToggle(Tabs.Main, "Auto Skip Level", false, function(v)
_G.AutoSkipLevel = v
while _G.AutoSkipLevel do
for _,v in ipairs(game.Players.LocalPlayer.Character:GetDescendants()) do
if v:IsA("BasePart") then
v.CanCollide = false
end
end
function Tween(Pos)
    Distance = (Pos.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
    if game.Players.LocalPlayer.Character.Humanoid.Sit == true then game.Players.LocalPlayer.Character.Humanoid.Sit = false end
    pcall(function() tween = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart,TweenInfo.new(Distance/30, Enum.EasingStyle.Linear),{CFrame = Pos}) end)
    tween:Play()
end

Tween(workspace.Checkpoints[game:GetService("Players").LocalPlayer.leaderstats.Stage.Value + 1].CFrame)
wait()
end
end)
